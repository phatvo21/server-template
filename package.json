{
  "name": "~~name~~",
  "version": "0.0.1",
  "description": "~~description~~",
  "author": "Phat Vo",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.20.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --no-cache --coverage --silent --passWithNoTests --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "lint-staged": "lint-staged",
    "start:prod:compose": "docker-compose up --abort-on-container-exit --build",
    "test:compose": "docker-compose -f ./docker-compose.testing.yaml up --remove-orphans --force-recreate --build -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phatvo21/~~name~~.git"
  },
  "bugs": {
    "url": "https://github.com/phatvo21/~~name~~/issues"
  },
  "homepage": "https://github.com/phatvo21/~~name~~/#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "nest-server-engine": "^1.0.3"
  },
  "devDependencies": {
    "@babel/register": "^7.21.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "eslint": "^8.0.1",
    "eslint-config-backend": "^1.0.3",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "jest-extended": "^3.2.4",
    "lint-staged": "^13.2.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  }
}
